package webdeveloper_one.doitjava;

public class For_2 {

	public static void main(String[] args) {
		// 2 * 1 = 2 2 * 2 = 4 ... 2 * 9 = 18
		// 3 * 1 = 3 3 * 2 = 6 ... 3 * 9 = 27
		
		for(int i = 2; i <= 9; i++) {
			for(int j = 1; j <= 9; j++) {
				System.out.print(i + " * " + j + " = " + i*j + "\t");
			}
			System.out.println();
		}
	}

}
