package webdeveloper_one.doitjava;

public class Opt_1 {

	public static void main(String[] args) {
		int init = 0;
		
		//++정수형변수 : 실행되면 + 1
		//정수형변수++ : 실행하고 또 실행하면 + 1
		System.out.println(++init);
		System.out.println(++init);
		System.out.println(++init);
		System.out.println(init);
		
		System.out.println(init++);
		System.out.println(init++);
		System.out.println(init++);
		System.out.println(init++);
		System.out.println(init);
	}

}
