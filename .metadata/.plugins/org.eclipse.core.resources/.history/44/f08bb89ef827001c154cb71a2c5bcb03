package webdeveloper_one.doitjava;

import java.util.Hashtable;

public class Hash_2 {

	public static void main(String[] args) {
		Hashtable hash = new Hashtable();
		hash.put(1, 1);
		hash.put(2, 2);
		hash.put(3, 3);
		hash.put(4, 4);
		hash.put(5, 5);
		
		/* for loop */
		for (int i = 0; i < hash.size(); i++) {
			System.out.println(hash.get(i + 1));
		}
	}

}
