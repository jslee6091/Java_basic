package webdeveloper_one.doitjava.child;

abstract class A{
	abstract void abc();
}

class B extends A{
	@Override
	void abc(){
		System.out.println("overrided method");
	}
}

public class AbstractClass_1 {

	public static void main(String[] args) {
		A a1 = new B();
		B b2 = new B();
		
		a1.abc();
		b2.abc();

	}

}
