package webdeveloper_one.doitjava;

public class Casting_1 {
	
	public static void main(String[] args) {
		
		// up , down casting
		// 강제, 자동 casting
		
		// 문자열이 제일 크다
		// 문자열이 크기 때문에 자동으로 정수형이 문자형으로 바뀐다
		System.out.println("문자열" + 10);
		// 문자 -> 정수
		String strvalue = "1";
		System.out.println(Integer.parseInt(strvalue) + 21);
	}
}
