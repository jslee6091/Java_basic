package webdeveloper_one.doitjava;

import java.io.BufferedReader;
import java.io.InputStreamReader;
import java.util.Enumeration;
import java.util.HashMap;
import java.util.Hashtable;

public class InputLogin_M {

	public static BufferedReader buffer = new BufferedReader(new InputStreamReader(System.in));
	
	public static void main(String[] args) {
		HashMap<String, Object> hash = new HashMap();
		
		new InputLoginBean_M("이재성", 26, "서울시", "aa@email.com", "재성");
		
		hash.put("이름", InputLoginBean_M.name);
		hash.put("나이", InputLoginBean_M.age);
		hash.put("주소", InputLoginBean_M.address);
		hash.put("이메일", InputLoginBean_M.email);
		hash.put("별명", InputLoginBean_M.nickname);
		
		for(String key : hash.keySet()) {
			System.out.println(hash.get(key));
		}
		
		Hashtable hast = new Hashtable(hash);
		
		Enumeration enu = hast.keys();
		
		while(enu.hasMoreElements()) {
			System.out.println(enu.nextElement());
		}

	}

}
