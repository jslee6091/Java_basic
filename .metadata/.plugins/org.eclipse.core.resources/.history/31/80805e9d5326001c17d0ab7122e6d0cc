package webdeveloper_one.doitjava;

public class For_2 {

	public static void main(String[] args) {
		// 2 * 1 = 2 2 * 2 = 4 ... 2 * 9 = 18
		// 3 * 1 = 3 3 * 2 = 6 ... 3 * 9 = 27
		
		for(int i = 2; i <= 9; i++) {
			for(int j = 1; j <= 9; j++) {
				System.out.print(i + " * " + j + " = " + i*j + "\t");
			}
			System.out.println();
		}
		
		// 2 x 1 3 x 1 4 x 1 ... 9 x 1
		// 2 x 2 3 x 2 4 x 2 ... 9 x 2
		// 3, 6, 8´Ü »©°í Ãâ·Â (ºó°ø°£ x)
		
		for(int i = 1; i <= 9; i++) {
			for(int j = 2; j <= 9; j++) {
				if(!(j == 3 || j == 6 || j == 8)) {
					System.out.print(j + " x " + i + " = " + i * j + "\t");
				}
			}
		}
	}

}
