package webdeveloper_one.doitjava.child;

public class CarManager {

	public static void main(String[] args) {
		// 현대, 기아, 삼성
		CarDealer car1 = new CarDealer();
		car1.Car("현대");
		car1.Car("기아");
		car1.Car("삼성");
		
		// 기아, 삼성
		CarDealer2 car2 = new CarDealer2();
//		car2.Car("현대차");
		car2.Car("기아");
		car2.Car("삼성");
		
		CommonCar common = new CarDealer();
		common.Car("현대");
		common.Car("기아");
		common.Car("삼성");
		
		
		CommonCar common2 = new CarDealer2();
		common2.Car("삼성");
		common2.Car("기아");
		

	}

}
